# editorconfig.org
root = true

[*]
indent_style = tab
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.{cs,js}]
indent_size = 4

[*.json]
indent_size = 4

[*.html]
indent_size = 4

[*.css]
indent_size = 4

[*.vue]
indent_size = 4

[*.handlebars]
indent_size = 4
