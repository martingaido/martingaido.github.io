{
	"manifest_version": 2,
	"name": "Sweeppea Milestones",
	"version": "4.0",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_icon": "icon-demo.png",
		"default_popup": "index.html",
		"default_title": "Hover text or content..."
	}
}
